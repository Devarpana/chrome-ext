{
    "manifest_version": 3,
    "name": "Alstylist",
    "version": "1.0.1",
    "description": "A Chrome extension helps you to buy product which is pocket friendly.",
    "action": {
        "default_popup": "/Frontend/HTML/index.html",
        "default_icon": {
            "16": "favicon.png",
            "32": "favicon.png",
            "128": "favicon.png"
        }
    },
    "permissions": ["activeTab"],
    "host_permissions": ["http://localhost:5173/*"]
}
